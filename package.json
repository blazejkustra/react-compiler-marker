{
  "name": "react-compiler-marker",
  "private": true,
  "description": "React Compiler Marker IDE extensions",
  "author": "Blazej Kustra <kustrablazej@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blazejkustra/react-compiler-marker"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint \"packages/*/src/**/*.ts\"",
    "format": "prettier --write \"packages/*/src/**/*.ts\"",
    "format:check": "prettier --check \"packages/*/src/**/*.ts\""
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "20.x",
    "@types/react": "^19.1.11",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "prettier": "3.7.4",
    "typescript": "^5.6.3"
  }
}
